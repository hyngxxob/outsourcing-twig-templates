function trimData() {
		var dropDown = $('.select2-selection__text');
			for(var i = 0; i < dropDown.length; i++) {
				//console.log(dropDown[i].innerText);
				if(dropDown[i].innerText.includes("1000개당")) {
					var str = $('.select2-selection__text')[i].innerText;
					var t1 = str.substring(0,str.indexOf("-"));
					//var t2_1 = t2.replace(",",".");
					//var t2_2 = t2_1.replace("00","원");
					//var t2_3 = t2_2.replace("1000개당", "1개당");
					
					var t2 = str.substring(str.indexOf("-"),str.length);
					var t2_1 = t2.replace("- ","");
					var t2_price = t2_1.replace(" 1000개당", "");
					var t2_price_result = trimPrice(t2_price);
		
					var result = t1 + t2_price_result + " 1개당";
					
					//console.log(result);
					//$('.select2-selection__text')[i].innerText = result;
				}
			}
	}
	
	function trimPrice(x) {
		var t1 = x.indexOf(",");
		var result = "";

		// 0.000
		if(t1 == 1) {
			result += x.substring(0,t1) + ".";
			result += x.substring(t1+1, t1+3) + "원";
		}
		// 00.000
		else if(t1 == 2) {
			result += x.substring(0,t1) + ".";
			result += x.substring(t1+1,t1+2) + "원";
		// 000.000
		} else if (t1 == 3) {
			result += x.substring(0,x.indexOf(",")) + "원";
		}
		return result;
	}

	setInterval(trimData, 0);
</script>
