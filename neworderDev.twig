				<strong style="margin: 16px;">상품 설명</strong>
				<div class="panel-body border-solid border-rounded" style="border: 0;">
					{{service['description']}}
				</div>
			</div>
		</div>
	</div>
	<div class="row" style="flex:1;">
		<div class="col-md-8 col-md-offset-2" style="width:100%;padding-right: 0; padding-left: 0;">
			<div class="well" style="padding: 0!important;">

				<form {% if site['rtl'] %} class="rtl-form" {% endif %} action="{{ page['url'] }}" method="post" id="order-form" style="border : 3px solid #F9A227; padding : 24px; border-radius: 32px; box-shadow: 0px 3px 20px #00000029;">
					{% if error %}
						<div class="alert alert-dismissible alert-danger {% if site['rtl'] %} rtl-alert {% endif %}">
							<button type="button" class="close" data-dismiss="alert">&times;</button>
							{{ errorMessage }}
						</div>
					{% endif %}

					{% if 1 == success %}
						<div class="alert alert-success alert-dismissible" role="alert">
							<button type="button" class="close" data-dismiss="alert" aria-label="Close">
								<span aria-hidden="true">&times;</span>
							</button>
							<h4>{{lang('neworder.message.success')}}</h4>
							{{lang('neworder.id')}}:
							{{order['id']}}<br>
							{{lang('neworder.service')}}:
							{{order['service']}}
							<br>
							{{lang('neworder.link')}}:
							{{order['link']}}<br>
							{{lang('neworder.quantity')}}:
							{{order['quantity']}}<br>
							{{ lang('neworder.charge') }}:
							{% if order.converted %}
								<span data-toggle="tooltip" data-placement="top" title="{{ order.original_charge }}">{{ order['charge'] }}</span>
							{% else %}
								{{ order['charge'] }}
							{% endif %}<br>
							{{ lang('neworder.balance') }}:
							{% if order.converted %}
								<span data-toggle="tooltip" data-placement="top" title="{{ order.original_balance }}">{{ order['balance'] }}</span>
							{% else %}
								{{ order['balance'] }}
							{% endif %}<br>
						</div>
					{% endif %}

						{% if 2 == success %}
							<div class="alert alert-success alert-dismissible" role="alert">
								<button type="button" class="close" data-dismiss="alert" aria-label="Close">
									<span aria-hidden="true">&times;</span>
								</button>
								<h4>{{lang('neworder.message.success')}}</h4>
								{{lang('neworder.subscription_id')}}:
								{{order['id']}}<br>
								{{lang('neworder.service')}}:
								{{order['service']}}
								<br>
								{{lang('neworder.username')}}:
								{{order['link']}}<br>
								{{lang('neworder.quantity')}}:
								{% if order['quantity_min'] == order['quantity_max'] %}
									{{order['quantity_max']}}
								{% else %}
									{{order['quantity_min']}}-{{order['quantity_max']}}
								{% endif %}
									<br>
									{{lang('neworder.new_posts')}}:
									{{order['posts']}}
									<br>
									{{lang('neworder.old_posts')}}:
									{{order['old_posts']}}
									<br>
									{{lang('neworder.delay')}}:
									{{order['delay']}}
									<br>
								</div>
							{% endif %}
								<div class="autoDescriptionWrapper">
									게시물 또는 영상 등 업로드 시 자동으로 작업이 진행됩니다. <br/>
									게시물(동영상) 주소가 아닌 <b style="color: #F9A227;">아이디</b>를 입력해 주세요.<br/>
								</div>
								<div class="sectionTabWrapper">
									<div class="sectionTab active" id='initClick'>주문하기</div>
									<div class="sectionTab">자동주문</div>
								</div>
								<div
									class="sectionContents">
									<!-- 탭 (인스타그램,인스타관리, 페이스북, 유튜브, 틱톡, 기타, 즐겨찾기) -->
									<div class="tabWrapper" style="display: none;">
										<div class="tab" onclick="filterOption('인스타그램')">
											<div class="tabIcon"><img src="https://cdn.mypanel.link/gz2yx6/fzymer8r6cz26tso.png"></div>
											인스타그램
										</div>
										<div class="tab" onclick="filterOption('인스타관리')" style="display: inline-block;">
											<div class="tabIcon"><img src="https://cdn.mypanel.link/gz2yx6/fzymer8r6cz26tso.png"></div>
											인스타관리
										</div>
										<div class="tab" onclick="filterOption('페이스북')">
											<div class="tabIcon"><img src="https://cdn.mypanel.link/gz2yx6/igog05of6wr7o9ly.png"></div>
											페이스북
										</div>
										<div class="tab active" onclick="filterOption('유튜브')">
											<div class="tabIcon"><img src="https://cdn.mypanel.link/gz2yx6/nwozdz6p37kxm7df.png"></div>
											유튜브
										</div>
										<div class="tab" onclick="filterOption('틱톡')">
											<div class="tabIcon"><img src="https://cdn.mypanel.link/gz2yx6/z4k1duu9oic6b74q.png"></div>
											틱톡
										</div>
										<div class="tab" onclick="filterOption('기타')">
											<div class="tabIcon"><img src="https://cdn.mypanel.link/gz2yx6/x6on3sywgq5m3lr0.png"></div>
											기타
										</div>
										<div class="tab" onclick="filterOption('VIP')">
											<div class="tabIcon"><img src="https://cdn.mypanel.link/gz2yx6/x6on3sywgq5m3lr0.png"></div>
											VIP&TEST
										</div>
									</div>
									{% if categories %}
										<div class="form-group">
											<label for="orderform-category" class="control-label">{{ lang('neworder.category') }}</label>
											<select class="form-control" id="orderform-category" name="OrderForm[category]" data-select="true" data-select-search="true" data-select-search-placeholder="{{ lang('general.search.placeholder') }}" {% if site['rtl'] %} data-select-dir="rtl" {% endif %}>
												{% for category in extended_categories %}
													<option value="{{ category.id }}" {% if category.icon %} data-icon="
														                          {% if category.icon['icon_type'] == 'icon' %}
														                            <span class='{{ category.icon['icon'] }}'></span>
														                          {% elseif category.icon['icon_type'] == 'emoji' %}
														                            <span>{{ category.icon['icon'] }}</span>
														                          {% elseif category.icon['icon_type'] == 'image' %}
														                            <img src='{{ category.icon['url'] }}' alt='{{ category.name }}' class='img-responsive btn-group-vertical'>
														                          {% endif %}
														                        " {% endif %} {% if category.id == data['category'] %} selected {% endif %}>
														{{ category.name }}
													</option>
												{% endfor %}
											</select>
										</div>
									{% endif %}

									<div class="form-group">
										<label for="orderform-service" class="control-label">{{ lang('neworder.service') }}</label><a href="#description" class="moveDiv">상품설명 &amp; 주의사항</a>
										<select class="form-control" id="orderform-service" name="OrderForm[service]" data-select="true" data-select-search="true" data-select-search-placeholder="{{ lang('general.search.placeholder') }}" {% if site['rtl'] %} data-select-dir="rtl" {% endif %}></select>
									</div>

									{# <div class="form-group hidden fields" id="service_description">
										<label for="service_description" class="control-label">{{ lang('neworder.description') }}</label>
										<div class="panel-body border-solid border-rounded">
											{{service['description']}}
										</div>
									</div> #}

									<div id="fields"></div>

									<div class="form-group">
										<label for="charge" class="control-label">{{ lang('neworder.charge') }}</label>
										<input type="text" class="form-control" id="charge" value="{{ order['charge'] }}" readonly>
									</div>

									{% if check_agreement %}
										<div class="form-group">
											<label class="control-label terms">
												<input type="checkbox" name="OrderForm[termsofservice]" class="terms-accept-checkbox" value="1" id="terms" {{data['termsofservice'] ? 'checked' : ''}}>
												{{lang('signup.accept_terms_text')}}
												<a href="{{ page_url('terms') }}" target="_blank">{{ lang('terms.signup_checkbox') }}</a>
											</label>
										</div>
									{% endif %}

										<input type="hidden" name="_csrf" value="{{ csrftoken }}">
										<button type="submit" id="submit-order" class="btn btn-primary" style="width:100%; background:#F9A227;" onclick="noauth()">{{ lang('neworder.button') }}</button>
									</form>
								</div>
							</div>
						</div>
					</div>
					{% if newOrderText %}
						<div class="container">
							<div class="row">
								<div class="col-md-8 col-md-offset-2">
									<div class="well {% if site['rtl'] %} rtl-content {% endif %}">
										{{ newOrderText }}
									</div>
								</div>
							</div>
						</div>
					{% endif %}
				</form>
			</div>
		</div>
	</div>
</div>
					
					
<script>
	var linkGuideContainer = document.getElementById('linkGuideContainer');
	var lG_Btn = linkGuideContainer.getElementsByClassName('linkGuideItem');
	for (var i = 0; i < lG_Btn.length; i++) {
		lG_Btn[i].addEventListener("click", function() {
			if(this.classList.contains('active')){
				this.classList.remove('active');
			}else{
				var current = document.getElementsByClassName("linkGuideItem active");
				if(current[0]){
					current[0].classList.remove('active');
				}
				this.classList.add('active');
			}
		});
	}
	let categoryList = [];
	$(window).load(function(){
		// iframe 값 가져오기
		var bagTest = [];
	
		var temp1 = $('#myFrame').contents().find('#serviceBag').html();
		var temp2 = temp1.trim();
		var serviceBag = temp2.split('\n');
		$('#servicesTest')[0].innerHTML = temp2;
		for(var i = 0 ; i < serviceBag.length; i++ ) {
			var obj = serviceBag[i].split('%col%');
			var it = new Object();
			it.id = obj[1].trim();
			it.name = obj[0].trim();
			it.icon = "null";
			categoryList.push(it);
		}
		$("#initClick").click();
		$(".tabWrapper").show();
	});

	const clock = document.querySelector('.h1-clock');


	function getTime(){
		const time = new Date();
		const hour = time.getHours();
		const minutes = time.getMinutes();
		const seconds = time.getSeconds();
		//clock.innerHTML = hour +":" + minutes + ":"+seconds;
		clock.innerHTML = `${hour<10 ? `0${hour}`:hour}:${minutes<10 ? `0${minutes}`:minutes}:${seconds<10 ? `0${seconds}`:seconds}`

		//const cloneTest = document.querySelector('#full-container-child2 > div > div.container > div.container > div > div > div').cloneNode(true)
        const notice = document.querySelector('#full-container-child2 > div > div.container > div.container > div > div > div').cloneNode(true);
        $('.noticeContentsWrapper').html(notice);
	}


	function init(){
		setInterval(getTime, 1000);
	}

	init();

	const TAB_ARRAY = [
	{
	"key": "instagram",
	"name": "\uc778\uc2a4\ud0c0\uadf8\ub7a8",
	"activeUrl": "https:\/\/cdn.mypanel.link\/gz2yx6\/26rsnaond90rngua.png",
	"url": "https:\/\/cdn.mypanel.link\/gz2yx6\/fzymer8r6cz26tso.png"
	},
	{
	"key": "instaedit",
	"name": "\uc778\uc2a4\ud0c0\uad00\ub9ac",
	"activeUrl": "https:\/\/cdn.mypanel.link\/gz2yx6\/26rsnaond90rngua.png",
	"url": "https:\/\/cdn.mypanel.link\/gz2yx6\/fzymer8r6cz26tso.png"
	},
	{
	"key": "facebook",
	"name": "\ud398\uc774\uc2a4\ubd81",
	"activeUrl": "https:\/\/cdn.mypanel.link\/gz2yx6\/29h2r1u0pmj93mg8.png",
	"url": "https:\/\/cdn.mypanel.link\/gz2yx6\/igog05of6wr7o9ly.png"
	},
	{
	"key": "youtube",
	"name": "\uc720\ud29c\ube0c",
	"activeUrl": "https:\/\/cdn.mypanel.link\/gz2yx6\/nwozdz6p37kxm7df.png",
	"url": "https:\/\/cdn.mypanel.link\/gz2yx6\/i60pguf7osxd0li3.png"
	}, {
	"key": "ticktalk",
	"name": "\ud2f1\ud1a1",
	"activeUrl": "https:\/\/cdn.mypanel.link\/gz2yx6\/3f096i6rcgodw2e4.png",
	"url": "https:\/\/cdn.mypanel.link\/gz2yx6\/z4k1duu9oic6b74q.png"
	}, {
	"key": "etc",
	"name": "\uae30\ud0c0",
	"activeUrl": "https:\/\/cdn.mypanel.link\/gz2yx6\/zxb4m7l6lw513h4i.png",
	"url": "https:\/\/cdn.mypanel.link\/gz2yx6\/x6on3sywgq5m3lr0.png"
	}, {
	"key": "vip",
	"name": "\u0056\u0049\u0050",
	"activeUrl": "https:\/\/cdn.mypanel.link\/gz2yx6\/zxb4m7l6lw513h4i.png",
	"url": "https:\/\/cdn.mypanel.link\/gz2yx6\/x6on3sywgq5m3lr0.png"
	}
	]

	$('div.autoDescriptionWrapper').hide();
	// 첫 랜딩 때, 주문하기 탭이 default여서 숨김처리

	// 주문하기, 자동주문 클릭
	$('div.sectionTab').click(function () {
	$('div.sectionTab').removeClass('active');
	$(this).addClass('active')

	// 자동주문일 때, 인스타관리 tab 삭제
	if ($(this).text() == '자동주문') {
	$('.tab:contains("인스타관리")').css('display', 'none');
	} else {
	$('.tab:contains("인스타관리")').css('display', 'inline-block');
	}

	// 첫번째 탭으로 초기화
	$('.tab').removeClass('active');
	$('.tab').first().addClass('active');
	$('.tab').children('.tabIcon').remove();

	$(".tab").each(function (index, item) {
	if ($(item).hasClass("active")) {
	$(item).prepend("<div class='tabIcon'><img src='" + TAB_ARRAY[index].activeUrl + "'></div>")
	} else {
	$(item).prepend("<div class='tabIcon'><img src='" + TAB_ARRAY[index].url + "'></div>")
	}
	})
	filterOption('인스타그램')


	// 자동주문일 때, 설명 toggle
	if ($(this).text() == '자동주문') {
	$('div.autoDescriptionWrapper').show();
	} else {
	$('div.autoDescriptionWrapper').hide();
	}
});

// 첫 loading 아이콘 삽입
