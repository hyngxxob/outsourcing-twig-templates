$('.sec7Container strong').on('click', function () { // 닫기
if ($(this).next().hasClass('active') && $(this).parent().hasClass('active')) {
$(this).next().removeClass('active');
$(this).parent().removeClass('active')

} else { // 열기
$('figure').removeClass('active');
$('div.figureDiv').removeClass('active');

$(this).next().addClass('active');
$(this).parent().addClass('active')
}
})


// 공지사항, 주문링크 탭 클릭
$('div.noticeTab').click(function () { // active 처리
	$('div.noticeTab').removeClass("active");
	$(this).addClass("active");

	// tab에 따라 contents 변경
	const type = $(this).text();
	if (type.includes('공지사항')) {
		$(".linkGuideWrapper").hide();
		$(".noticeContentsWrapper").show();
	} else {
		$(".noticeContentsWrapper").hide();
		$(".linkGuideWrapper").show();
	}
})

