<script>
    document.getElementsByClassName('userImage')[0].addEventListener("click", function (){
        if(this.classList.contains('active')){
            this.classList.remove('active');
        }else{
            var current = document.getElementsByClassName("faq-item active");
            if(current[0]){
                current[0].classList.remove('active');
            }
            this.classList.add('active');
        }
    });
    
