        alert("홈 화면의 '둘러보기' 버튼을 눌러주세요!");
        window.location.href = "/";
    }

    function collapseNav() {
        if(!document.getElementById('full-container-child1').hasAttribute("style","")) {
            document.getElementById('full-container-child1').style.width = "100px";
            document.getElementById('text-logo').style.display = "none";
        } else {
            document.getElementById('full-container-child1').removeAttribute("style","");
            document.getElementById('text-logo').style.display = "initial";
        }
    }
</script>